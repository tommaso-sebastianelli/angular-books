<?xml version="1.0" encoding="UTF-8" ?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file source-language="en" datatype="plaintext" original="ng2.template">
    <body>
      <trans-unit id="search-page-title" datatype="html">
        <source>Search</source>
        <target>Ricerca</target>
      </trans-unit>
      <trans-unit id="loans-page-title" datatype="html">
        <source>Loans</source>
        <target>Prestiti</target>
      </trans-unit>
      <trans-unit id="loan-button" datatype="html">
        <source>LOAN</source>
        <target>PRESTITO</target>
      </trans-unit>
      <trans-unit id="card-author" datatype="html">
        <source>Author</source>
        <target>Autore</target>
      </trans-unit>
      <trans-unit id="card-editor" datatype="html">
        <source>Editor</source>
        <target>Editore</target>
      </trans-unit>
      <trans-unit id="search-placeholder-image" datatype="html">
        <source>a round magnifying glass icon</source>
        <target>un'icona tonda di una lente di ingrandimento</target>
      </trans-unit>
      <trans-unit id="search-placeholder-title" datatype="html">
        <source>Find by keyword</source>
        <target>Inserisci una parola chiave</target>
      </trans-unit>
      <trans-unit id="search-placeholder-subtitle" datatype="html">
        <source>Search by name or author, over millions of books</source>
        <target>Cerca in base a nome o autore, tra milioni di libri</target>
      </trans-unit>
      <trans-unit id="loans-placeholder-image" datatype="html">
        <source>a round book icon</source>
        <target>un'icona tonda di un libro</target>
      </trans-unit>
      <trans-unit id="loans-placeholder-title" datatype="html">
        <source>No Loans yet</source>
        <target>Non hai ancora nessun prestito</target>
      </trans-unit>
      <trans-unit id="loans-placeholder-subtitle" datatype="html">
        <source>Loan your first book on the search page</source>
        <target>Chiedi il tuo primo prestito tramite la pagina di ricerca</target>
      </trans-unit>
      <trans-unit id="generic-cancel" datatype="html">
        <source>CANCEL</source>
        <target>ANNULLA</target>
      </trans-unit>
      <trans-unit id="duration" datatype="html">
        <source>Duration</source>
        <target>Durata</target>
      </trans-unit>
      <trans-unit id="loan-dialog-title" datatype="html">
        <source>Set loan duration</source>
        <target>Scegli la durata del prestito</target>
      </trans-unit>
      <trans-unit id="loan-dialog-subtitle" datatype="html">
        <source>Loan expire time is calculated after the library confirms your loan</source>
        <target>La data di fine prestito verrà calcolata non appena la biblioteca confermerà la richiesta</target>
      </trans-unit>
      <trans-unit id="week-plural" datatype="html">
        <source>{slider.displayValue, plural, =1 {Week} other {Weeks}}</source>
        <target>{slider.displayValue, plural, =1 {Settimana} other {Settimane}}</target>
      </trans-unit>
      <trans-unit id="loan-success-message" datatype="html">
        <source>Book loaned!</source>
        <target>Libro prenotato!</target>
      </trans-unit>
      <trans-unit id="undo" datatype="html">
        <source>UNDO</source>
        <target>ANNULLA</target>
      </trans-unit>
      <trans-unit id="search-history" datatype="html">
        <source>Recent searches:</source>
        <target>Ricerche recenti:</target>
      </trans-unit>
    </body>
  </file>
</xliff>
